<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to "Entrar com #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class:"waves-effect waves-light btn blue"%>
  <% end -%>
<% end -%>

<br/>
<br />
<%- if controller_name != 'sessions' %>
  <%= link_to "Entrar", new_session_path(resource_name)%>
<% end -%>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Registrar-se", new_registration_path(resource_name)%>
<% end -%>